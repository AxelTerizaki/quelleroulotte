    "google-spreadsheet": "^3.3.0",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/google-spreadsheet": "^3.3.1",
    "@types/lodash": "^4.14.191",
